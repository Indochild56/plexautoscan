{
   "DOCKER_NAME":"plex",
   "GOOGLE": {
     "ENABLED": false,
     "CLIENT_ID": "{{gdriveid.stdout}}",
     "CLIENT_SECRET": "{{gdrivesecret.stdout}}",
     "ALLOWED": {
       "FILE_PATHS": [
         "My Drive/Movies/",
         "My Drive/TV/",
         "My Drive/Music/"
       ],
       "FILE_EXTENSIONS": true,
       "FILE_EXTENSIONS_LIST": [
         "webm","mkv","flv","vob","ogv","ogg","drc","gif",
         "gifv","mng","avi","mov","qt","wmv","yuv","rm",
         "rmvb","asf","amv","mp4","m4p","m4v","mpg","mp2",
         "mpeg","mpe","mpv","m2v","m4v","svi","3gp","3g2",
         "mxf","roq","nsv","f4v","f4p","f4a","f4b","mp3",
         "flac","ts"
       ],
       "MIME_TYPES": true,
       "MIME_TYPES_LIST": [
         "video"
       ]
     },
     "TEAMDRIVE": false,
     "TEAMDRIVES": [
       "Name of the Team Drive"
     ],
     "POLL_INTERVAL": 60,
     "SHOW_CACHE_LOGS": false
   },
   "PLEX_ANALYZE_DIRECTORY": true,
   "PLEX_ANALYZE_TYPE": "deep",
   "PLEX_FIX_MISMATCHED": {{fixmatchstatus.stdout}},
   "PLEX_FIX_MISMATCHED_LANG": "{{fixmatchlang.stdout}}",
   "PLEX_DATABASE_PATH": "/plexDb/com.plexapp.plugins.library.db",
   "PLEX_EMPTY_TRASH": true,
   "PLEX_EMPTY_TRASH_CONTROL_FILES":[
      "{{path.stdout}}/unionfs/mounted.bin"
   ],
   "PLEX_EMPTY_TRASH_MAX_FILES": 100,
   "PLEX_EMPTY_TRASH_ZERO_DELETED": true,
   "PLEX_LD_LIBRARY_PATH": "/usr/lib/plexmediaserver/lib",
   "PLEX_LOCAL_URL": "http://plex:32400",
   "PLEX_SCANNER": "/usr/lib/plexmediaserver/Plex\\ Media\\ Scanner",
   "PLEX_SUPPORT_DIR": "{{plex_lib_folder.stdout}}",
   "PLEX_USER": "{{plex_docker_user.stdout}}",
   "PLEX_TOKEN": "{{plex_auth_token.stdout}}",
   "PLEX_CHECK_BEFORE_SCAN": true,
   "PLEX_WAIT_FOR_EXTERNAL_SCANNERS": false,
   "RCLONE": {
    "BINARY": "/usr/bin/rclone",
    "CONFIG": "/config/rclone/rclone.conf",
    "CRYPT_MAPPINGS": {
    },
    "RC_CACHE_REFRESH": {
      "ENABLED": false,
      "FILE_EXISTS_TO_REMOTE_MAPPINGS": {
      "Media/": [
          "{{path.stdout}}/Media/"
      ]
      },
      "RC_URL": "http://localhost:5572"
    }
  },
   "RUN_COMMAND_BEFORE_SCAN":"",
   "RUN_COMMAND_AFTER_SCAN": "",
   "SERVER_ALLOW_MANUAL_SCAN": true,
   "SERVER_FILE_CHECK_DELAY": 30,
   "SERVER_MAX_FILE_CHECKS": 5,
   "SERVER_FILE_EXIST_PATH_MAPPINGS":{
     "{{path.stdout}}/": [
       "{{path.stdout}}/"
     ]
   },
   "SERVER_IGNORE_LIST":[
      "Thumbs.db"
   ],
   "SERVER_IP": "0.0.0.0",
   "SERVER_PASS": "{{plex_autoscan_server_pass.stdout}}",
   "SERVER_PATH_MAPPINGS":{
     "{{path.stdout}}/unionfs/": [
       "/home/user/media/fused/"
     ]
   },
   "SERVER_PORT": 3468,
   "SERVER_SCAN_DELAY": 20,
   "SERVER_SCAN_FOLDER_ON_FILE_EXISTS_EXHAUSTION": true,
   "SERVER_SCAN_PRIORITIES":{
   },
   "SERVER_USE_SQLITE": true,
   "USE_DOCKER": true,
   "USE_SUDO": false
}
